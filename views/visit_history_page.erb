<div class="sub_section_title">Patient visit history
  </div>
<div id="visit_history">
    <div id="show_new_visit_form" class="hyperlinks">
      <a href="/myClinic/getUpdateHistoryForm?id=<%=@id%>" location="#record_visit_section">Update history</a>
    </div>
    <table id="visit_history_table" class="general_table scrollable_table">
    <%
    unless @no_history then
      %>
      <thead>
        <tr id="header_row">
        <%@history_headers.each do |header|%>
          <th><%=header%></th>
          <%end%>
        </tr>
      </thead>
      <tbody>
      <%@visit_history_for_display.each do |history|%>
          <tr>
          <%@history_keys.each do |key|%>
            <td><%=history.fetch_entry_value(key)%></a></td>
            <%end%>
          </tr>
          <%
        end
      end
      %>
      </tbody>
    </table>
</div>