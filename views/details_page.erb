<div id="patient_basic_information_section" class="rounded_bordered_elements">
<div class="section_title">Detailed information
  </div>
  <div id="patient_details" >
  <table id="patient_details_table" class="borderless_table">
    <%
    keys = @details_for_display.keys
    keys.each do |key|
    %>
    <tr>
      <td><%=key %></td>
      <td><%=@details_for_display[key] %></td>
    </tr>
    <%end%>
  </table>
</div>
<br/>
<div id="details_hyperlinks" class="hyperlinks">
  <a href="/myClinic/visitHistory?id=<%=@id%>" location="#visit_history_section">Show history</a>
  <a href="/myClinic/getUpdatePatientDetailsForm?id=<%=@id%>" location="#patient_basic_information_section">Update</a>
</div>
<br/>
</div>
<div id="record_visit_section" class="rounded_bordered_elements"></div>
<div id="visit_history_section" class="rounded_bordered_elements"></div>